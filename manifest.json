{
  "manifest_version": 2,

  "name": "web.Glitch()",
  "version": "1.0.1",
  "description": "do glitch on web",

  "short_name": "web.Glitch()",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

   "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["jquery.js", "wobble.js"],
        "run_at": "document_end"
        }
    ], 
  "permissions": [
     "http://*/*", "https://*/*"
  ]
}
