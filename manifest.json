{
  "manifest_version": 2,

  "name": "web3000",
  "description": "This extension glitches the web",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

   "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["jquery.js", "wobble.js"],
        "run_at": "document_end"
        }
    ], 
  "permissions": [
    "https://secure.flickr.com/"
  ]
}
